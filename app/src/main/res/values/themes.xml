<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.MartMobile" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
         <item name="colorPrimary">@color/brown</item>
        <item name="colorPrimaryVariant">@color/brown</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/brown</item>
        <item name="colorSecondaryVariant">@color/brown</item>
        <item name="colorOnSecondary">@color/black</item>
        <color name="textColorDark">@color/black</color>
        <item name="android:colorControlHighlight">@android:color/transparent</item>

    </style>

    <style name="Theme.MartMobile" parent="Base.Theme.MartMobile" />


    <style name="Base.Theme.MartMobile2" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
         <item name="colorPrimary">@color/brown</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/brown</item>

    </style>

    <style name="Theme.MartMobile2" parent="Base.Theme.MartMobile2" />
</resources>